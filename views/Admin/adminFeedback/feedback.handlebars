{{#if success}}
  <script>
    alert("Feedbacks deleted"); window.location.href =
    '/guest/feedback';
  </script>
{{/if}}

<p style="margin-left: 30px;">Select feedbacks to be deleted:</p>
<form id="deletionForm" action="/admin/feedback">
 <div id="error-container">{{#if error}}{{error}}{{/if}}</div>
  <ul>
    {{#if feedback.length}}
      {{#each feedback}}
        <li class="feedback-card" style="width: 50%;">
          <h2>Guest: {{guestName}}</h2>
          Room:
          {{roomType}}&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
          rating :
          {{rating}}/10
          <input
            type="checkbox"
            name="deleteFeedbackIds"
            class="deleteFeedbackIds"
            value="{{_id}}"
          /><span style="float: right;">Flag for deletion : </span>
          <p>{{comment}}</p>
          
        </li>
      {{/each}}
      </ul>
       <button
    type="submit"
    id="deleteButton"
    onclick="deleteFlaggedFeedback()"
  >Delete flagged feedback</button>
    {{else}}
      <h2>No feedback available</h2>
      
    {{/if}}
  
 

<div class="space-below">

</div>
<script src="../../../public/js/main.js"></script>
<footer>

  <a href="/admin">back to admin Dashboard</a>
</footer>